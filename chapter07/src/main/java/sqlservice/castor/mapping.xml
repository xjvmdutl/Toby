<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapping PUBLIC "-//EXOLAB/Castor Mapping DTD Version 1.0//EN" "http://castor.org/mapping.dtd">
<mapping>
  <class name="sqlservice.jaxb.Sqlmap">
    <map-to xml="sqlmap" />
    <field name="sql" type="sqlservice.jaxb.SqlType"
      required="true" collection="arrayList">
      <bind-xml name="sql" node="element" />
    </field>
  </class>
  <class name="sqlservice.jaxb.SqlType">
    <map-to xml="sql" />
    <field name="key" type="string" required="true">
      <bind-xml name="key" node="attribute" />
    </field>
    <field name="value" type="string" required="true">
      <bind-xml node="text" />
    </field>
  </class>
</mapping>